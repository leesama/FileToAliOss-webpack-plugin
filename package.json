    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.6",
    "@types/ali-oss": "^6.16.8",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.4",
    "jest": "^29.6.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "ali-oss": "^6.17.1",
    "chalk": "4.1.2",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leesama/FileToAliOss-webpack-plugin.git"
